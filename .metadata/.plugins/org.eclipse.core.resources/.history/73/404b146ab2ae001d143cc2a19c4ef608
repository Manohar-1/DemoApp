package com.product.model;

public class Pen extends Product {
     
	private String color;

	public Pen() {
		
	}
	
	public Pen(String color) {
		super();
		this.color = color;
	}

	public String getColor() {
		return color;
	}

	public void setColor(String color) {
		this.color = color;
	}

	@Override
	public String toString() {
		return "Pen [color=" + color + "]";
	}
	
	
	
}
